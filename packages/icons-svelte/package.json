{
	"name": "@webisopen/icons-svelte",
	"version": "0.0.22",
	"description": "Svelte Icons for Open Web",
	"license": "MIT",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./icons": {
			"types": "./dist/icons.d.ts",
			"svelte": "./dist/icons.js",
			"default": "./dist/icons.js"
		},
		"./icons/*": {
			"types": "./dist/icons/*.svelte.d.ts",
			"svelte": "./dist/icons/*.svelte",
			"default": "./dist/icons/*.svelte"
		}
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"sideEffects": false,
	"files": ["dist"],
	"scripts": {
		"build": "node ./scripts/build.mjs"
	},
	"devDependencies": {
		"svelte": "^5.0.0",
		"@sveltejs/package": "^2.3.7"
	},
	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"svelte": ">= 5"
	},
	"peerDependenciesMeta": {
		"svelte": {
			"optional": true
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/webisopen/icons.git",
		"directory": "packages/icons-svelte"
	},
	"homepage": "https://icons.webisopen.com",
	"bugs": {
		"url": "https://github.com/webisopen/icons/issues"
	}
}
